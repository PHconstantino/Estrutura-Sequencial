Algoritmo "Exercicio Consumo"


Var

           distancia, medio, combustivel: real

Inicio

        escreva ("Distancia percorrida: ")
        leia(distancia)
        escreva ("Combustível gasto: ")
        leia (combustivel)
        
        medio <- distancia / combustivel
        
        escreval ("Consumo medio = ", medio:4:3)


Fimalgoritmo